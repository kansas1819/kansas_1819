[School Name]
; truncate at 65 chars
trunc = 65
field = SCHOOL_NAME

[Student Name]
; truncate at 60 chars
trunc = 65
field = STU_NAME

[School Id]
; check for length and flag if > or <
length = 10
field = SCHOOL_ID

[Student Id]
; check for length <= 9 or flag
length = 9
field = CHILD_ENROLL_NO

[Standard]
; flag if > 1 chars
length = 1
field = STANDARD